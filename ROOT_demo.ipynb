   "source": [
    "import ROOT\n",
    "\n",
    "# Create a histogram\n",
    "hist = ROOT.TH1F(\"hist\", \"My First ROOT Histogram\", 100, -5, 5)\n",
    "hist.FillRandom(\"gaus\", 10000)  # Fill with random Gaussian data\n",
    "\n",
    "# Draw it\n",
    "canvas = ROOT.TCanvas()\n",
    "hist.Draw()\n",
    "canvas.Draw()"
   ]
  }
